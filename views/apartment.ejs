      <div class="info-text">
        <div class="project-info"><span><b>Project Info</b></span></div>
        <ul class="info-list">
          <li class="info-item">
            <span><b>Category:</b> Residential Apartment</span>
          </li>
          <li class="info-item">
            <span><b>Total Flats:</b><%= locals.owner_name || 'Default_Ownername' %></span>
          </li>
          <% if(locals.completed_on !== null) {%>
            <li class="info-item">
              <span><b>Completed On: </b><%= locals.completed_on.toLocaleDateString() || 'Default_Completed_on' %></span>
            </li>
          <% } %>
        </ul>
      </div>

      <button class="button">
        <div class="icon">
          <div class="arrow"></div>
        </div>
          &#66;rochure
      </button>


    </div>
    
    <div class="img_window">
      <div class="img_container">
        <img class="img" src="https://d3hne3c382ip58.cloudfront.net/files/uploads/bookmundi/resized/cmsfeatured/holland-tour-highlights-1501738379-1000X561.jpg">
      </div>
    </div>   
