            <img class="web_slide-thumbnail" src="https://images.unsplash.com/photo-1485199433301-8b7102e86995?ixlib=rb-0.3.5&q=85&fm=jpg&crop=entropy&cs=srgb&ixid=eyJhcHBfaWQiOjE0NTg5fQ&s=c7783fe3a697b1a2248450120435cbc3" onclick="web_currentSlide(6)" alt="Caption Six">
          </div>
        </div>
      </div> 
      <div class="column-flex row2">   
        <div class="address-container">
          <div class="address-title"><span><b>ADDRESS</b></span></div>
          <span class="apartment-address"><%= locals.apt_address || 'Default_Aptaddress' %></span>
        </div>
        <div class="map-container">
          <iframe src = <%= locals.gmap_link || 'Default_Gmaplink' %> style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade" class="map"></iframe> 
        </div>  
      </div> 
    </div>  
  </div> 
</section>

<footer class="footer">
  <div class="main-footer">
    <div class="contact-details">
      <h1>Contact Us</h1>
      <li>
        <div class="fa fa-phone"></div><a href="tel:+919326048690">+91 0987654321</a></li>
      <li>
        <div class="fa fa-envelope"></div><a href="mailto:yourmail@gmail.com">yourmail@gmail.com</a></li>
      </li>
    </div>

    <div class="info" data-aos="fade-up">
      <h1>Social Media</h1>
      <div class="sociallogos">
        <div class="logobox">
          <a href="#" class="fa fa-instagram"></a>
          <a href="#" class="fa fa-whatsapp"></a>
          <a href="#" class="fa fa-facebook"></a>
        </div>
      </div>
    </div>
  </div>
  <div class = "copyrights"><p>© Copyright 2021 All Rights Reserved</p></div>
</footer>


<!--========== SCROLL REVEAL ==========-->
